{
    "manifest_version": 2,
    "name": "Readio",
    "version": "0.01",
    "content_scripts": [
            {
                "matches": [
                    "http://*/*",
                    "https://*/*"
                    ],
                "js": ["content.js"]
            }
        ],
        "background": {
            "scripts": ["background.js"]
        },
        "browser_action": {
            "default_icon": "book.png",
            "default_title": "readio",
            "default_popup": "index.html"
        }
}